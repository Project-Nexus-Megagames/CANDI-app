{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
		"rules": {
			"no-console": "off",
			"no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
			"no-constant-condition": "off",
			"no-case-declarations": "off",
			"no-mixed-spaces-and-tabs": "warn",
			"max-len": "off"
		}
}
